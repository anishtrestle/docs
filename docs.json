{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Trestle Identity APIs",
  "theme": "mint",
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/assets/trestle_logo.png",
    "dark": "/assets/trestle_logo.png",
    "href": "https://trestleiq.com"
  },
  "colors": {
    "primary": "#163545",
    "light": "#0694B4",
    "dark": "#1E3F51"
  },
  "appearance": {
    "default": "system",
    "strict": false
  },
  "fonts": {
    "heading": {
      "family": "Inter"
    },
    "body": {
      "family": "Inter"
    }
  },
  "styling": {
    "codeblocks": {
      "theme": {
        "light": "github-light",
        "dark": "github-dark"
      }
    }
  },
  "api": {
    "mdx": {
      "server": "https://api.trestleiq.com",
      "auth": {
        "method": "key",
        "name": "x-api-key"
      }
    }
  },
  "settings": {
    "customStyles": [
      "/style.css"
    ]
  },
  "navigation": {
    "versions": [
      {
        "version": "Current",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "guides/overview",
              "guides/authentication"
            ]
          },
          {
            "group": "API Reference",
            "pages": [
              {
                "group": "Reverse Phone API",
                "expanded": false,
                "pages": [
                  "api-reference/reverse-phone-api"
                ]
              },
              {
                "group": "Caller Identification API",
                "expanded": false,
                "pages": [
                  "api-reference/caller-identification-api"
                ]
              },
              {
                "group": "Smart CNAM API",
                "expanded": false,
                "pages": [
                  "api-reference/smart-cnam-api"
                ]
              },
              {
                "group": "Phone Validation API",
                "expanded": false,
                "pages": [
                  "api-reference/phone-validation-api"
                ]
              },
              {
                "group": "Reverse Address API",
                "expanded": false,
                "pages": [
                  "api-reference/reverse-address-api"
                ]
              },
              {
                "group": "Real Contact API",
                "expanded": false,
                "pages": [
                  "api-reference/real-contact-api"
                ]
              }
            ]
          }
        ],
        "default": true
      },
      {
        "version": "Archived",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "intro",
              "guides/overview",
